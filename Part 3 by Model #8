'KVF650 Brute Force 4x4i',
'KVF750 Brute Force 4x4i',
'KVF750 Brute Force 4x4i EPS',
'KVF750 Brute Force 4x4i EPS Camo',
'LT-A700X KingQuad 4x4',


]
years = list(range(2003, 2024))

# Step 5: Generate the cable DataFrame
generator = BootGenerator(boot_template, list_models, years)
final_df = generator.generate_boot()

